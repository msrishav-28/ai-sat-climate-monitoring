# Rename this file to secrets.toml and add your credentials
# DO NOT commit the actual secrets.toml file to git

[gee]
service_account = "your-service-account@your-project.iam.gserviceaccount.com"
private_key = """-----BEGIN PRIVATE KEY-----
YOUR_PRIVATE_KEY_HERE
-----END PRIVATE KEY-----"""

[mapbox]
token = "your-mapbox-token-here"  # Optional for custom map styles

[api]
key = "your-api-key-here"  # If exposing REST endpoints